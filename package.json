{
  "name": "montydawsonv2",
  "version": "1.0.1",
  "description": "My personal website",
  "main": "public/index.html",
  "scripts": {
    "build": "rimraf public_html && webpack --bail --progress --profile",
    "dev": "webpack-dev-server --hot --inline",
    "serve": "npm run build && node node_modules/http-server/bin/http-server public_html",
    "start": "npm run serve",
    "test": "echo No Tests Yet",
    "test-watch": "node ./node_modules/jest/bin/jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MontyD/MontyDawsonV2.git"
  },
  "keywords": [
    "Personal",
    "Website",
    "Monty",
    "Dawson"
  ],
  "author": "MontyDawson <hello@montydawson.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MontyD/MontyDawsonV2/issues"
  },
  "homepage": "https://github.com/MontyD/MontyDawsonV2#readme",
  "devDependencies": {
    "@types/jest": "~22.0.1",
    "@types/node": "~8.5.7",
    "@types/react": "~16.0.34",
    "@types/react-dom": "~16.0.3",
    "@types/react-router-dom": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/tests-preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|js)"
    ]
  },
  "dependencies": {
    "autoprefixer": "~7.2.4",
    "awesome-typescript-loader": "~3.4.1",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-preset-env": "~1.6.1",
    "caniuse-db": "~1.0.30000787",
    "copy-webpack-plugin": "~4.3.1",
    "css-loader": "~0.28.8",
    "cssnano": "~3.10.0",
    "extract-text-webpack-plugin": "~3.0.2",
    "favicons-webpack-plugin": "~0.0.7",
    "file-loader": "~1.1.6",
    "html-webpack-inline-source-plugin": "~0.0.9",
    "html-webpack-plugin": "~2.30.1",
    "http-server": "~0.10.0",
    "jest": "~22.0.4",
    "node-sass": "~4.7.2",
    "offline-plugin": "~4.9.0",
    "postcss-loader": "~2.0.10",
    "raw-loader": "~0.5.1",
    "react": "~16.2.0",
    "react-bem-helper": "~1.4.1",
    "react-dom": "~16.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "~2.6.2",
    "sass-loader": "~6.0.6",
    "serviceworker-webpack-plugin": "~0.2.3",
    "style-loader": "~0.19.1",
    "tslint": "~5.4.3",
    "tslint-react": "^3.3.3",
    "typescript": "~2.6.2",
    "webpack": "~3.10.0",
    "webpack-dev-server": "~2.10.0"
  }
}
