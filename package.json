{
  "name": "montydawsonv2",
  "version": "1.0.1",
  "description": "My personal website",
  "main": "public/index.html",
  "scripts": {
    "build": "rimraf public_html && webpack --bail --progress --profile",
    "dev": "webpack-dev-server --hot --inline",
    "serve": "npm run build && node node_modules/http-server/bin/http-server public_html",
    "start": "npm run serve",
    "test": "node ./node_modules/jest/bin/jest",
    "test-watch": "node ./node_modules/jest/bin/jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MontyD/MontyDawsonV2.git"
  },
  "keywords": [
    "Personal",
    "Website",
    "Monty",
    "Dawson"
  ],
  "author": "MontyDawson <hello@montydawson.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MontyD/MontyDawsonV2/issues"
  },
  "homepage": "https://github.com/MontyD/MontyDawsonV2#readme",
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.12",
    "autoprefixer": "6.5.4",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-env": "^1.6.1",
    "caniuse-db": "^1.0.30000649",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "0.9.0",
    "html-webpack-inline-source-plugin": "0.0.6",
    "html-webpack-plugin": "2.24.1",
    "http-server": "0.9.0",
    "jest": "^19.0.2",
    "node-sass": "4.1.1",
    "offline-plugin": "4.5.3",
    "postcss-loader": "1.2.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "serviceworker-webpack-plugin": "0.1.7",
    "style-loader": "0.13.1",
    "tslint": "^5.4.3",
    "tslint-config-airbnb": "^5.2.1",
    "tslint-loader": "^3.5.3",
    "typescript": "2.2.2",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/tests-preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|js)"
    ]
  },
  "dependencies": {
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "react": "^16.2.0",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^16.2.0"
  }
}
